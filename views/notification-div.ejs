/* Notification CSS Start */

.moxo_bar {
    width: 100%;
    color: #fff;
    overflow: hidden;
    left: 0;
    right: 0;
    margin: auto;
	text-align: center;
    z-index: 0;
	min-height: 50px;
}

.moxo_holder {
    position: relative;
    display: inline-block;
    width: auto;
    padding: .5em 0;
}

.moxo_message {
	font-weight: 500;
    -webkit-font-smoothing: auto;
    word-wrap: break-word;
    max-width: 88%;
